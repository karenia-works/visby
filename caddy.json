{
  "apps": {
    "http": {
      "http_port": 80,
      "https_port": 443,
      "servers": {
        "srv0": {
          "listen": [":2015"],
          "routes": [
            {
              "match": [
                {
                  "path": ["/api/account"]
                }
              ],
              "handle": [
                {
                  "handler": "subroute",
                  "routes": [
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-paper:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/api/paper"]
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-professor:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/api/professor"]
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-user:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/api/user"]
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-account:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/connect"]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ],
          "experimental_http3": true
        },
        "srv1": {
          "listen": [":80"],
          "routes": [
            {
              "match": [
                {
                  "host": ["visby-account"]
                }
              ],
              "handle": [
                {
                  "handler": "subroute",
                  "routes": [
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-paper:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/api/paper"]
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-professor:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/api/professor"]
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-user:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/api/user"]
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-account:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/connect"]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ],
          "experimental_http3": true
        },
        "srv2": {
          "listen": [":443"],
          "routes": [
            {
              "match": [
                {
                  "host": ["reverse_proxy"]
                }
              ],
              "handle": [
                {
                  "handler": "subroute",
                  "routes": [
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-paper:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/api/paper"]
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-professor:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/api/professor"]
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-user:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/api/user"]
                        }
                      ]
                    },
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "visby-account:80"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": ["/connect"]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ],
          "experimental_http3": true
        }
      }
    }
  }
}
